@model IEnumerable<OECS.Models.CategoryModels.CategoryModel>

@if (Model.Count() > 0)
{
    <li class="nav-item active">
        <a class="nav-link category" categoryID="0">
            All
            <span class="sr-only">(current)</span>
        </a>
    </li>

    foreach (var category in Model)
    {
        <li class="nav-item">
            <a class="nav-link category" categoryID="@category.CategoryID">@category.Category</a>
        </li>
    }
}
else
{
    <li class="nav-item">
        <a class="nav-link" href="#">No Category Available</a>
    </li>
}